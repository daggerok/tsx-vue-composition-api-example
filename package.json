{
  "name": "tsx-vue-composition-api-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "gh": "cross-env BASE_HREF='/tsx-vue-composition-api-example/' npm run build",
